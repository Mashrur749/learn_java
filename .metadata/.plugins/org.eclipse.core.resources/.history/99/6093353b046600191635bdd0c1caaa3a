	public static int getAllSum(int[] a) {
		int val = 0;
		for(int x: a)
			val+=x;
		return val;
	}
	
	static int[] getNext2(int x) {
		int[] vals = new int[2];
		vals[0] = x + 1;
		vals[1] = x + 2;
		return vals;
	}
 
		
	public static void main(String[] args) {
		int incMe = 0;
		System.out.println("incMe: "+(incMe++));
		System.out.println("incMe: "+(++incMe));
	}
}